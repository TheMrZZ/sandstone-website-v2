{
  "name": "nextjs-notion-starter-kit",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser ANALYZE=true next build",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "posttest": "run-s build"
  },
  "dependencies": {
    "@notionhq/client": "^0.4.12",
    "classnames": "^2.3.1",
    "fathom-client": "^3.3.1",
    "got": "^12.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "lqip-modern": "^1.2.0",
    "next": "^12.0.8",
    "node-fetch": "^3.2.0",
    "notion-client": "^4.14.1",
    "notion-types": "^4.14.1",
    "notion-utils": "^4.14.1",
    "p-map": "^5.3.0",
    "p-memoize": "^6.0.1",
    "react": "^17.0.2",
    "react-body-classname": "^1.3.1",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-notion-x": "^4.14.2",
    "react-use": "^17.3.2",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.8",
    "@types/classnames": "^2.3.1",
    "@types/node": "^17.0.10",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.38",
    "@types/react-burger-menu": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vercel": "^23.1.2"
  }
}
